{
  "expo": {
    "name": "ForkIt",
    "slug": "forkit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "forkit",
    "newArchEnabled": true,
    "description": "Fork indecision. Random restaurant picker with copycat recipes. Just tap and we'll choose for you.",
    "privacy": "https://CherrelleTucker.github.io/forkit/privacy.html",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.forkit.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ForkIt needs your location to find nearby restaurants and help you decide where to eat.",
        "NSLocationAlwaysUsageDescription": "ForkIt needs your location to find nearby restaurants and help you decide where to eat."
      },
      "config": {
        "googleMapsApiKey": ""
      }
    },
    "android": {
      "package": "com.forkit.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0B0F"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "ForkIt needs your location to find nearby restaurants and help you decide where to eat."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0abe90d5-587c-4918-ad8b-7d472c687ace"
      }
    },
    "owner": "cjtucker"
  }
}
